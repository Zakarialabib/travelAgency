{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "artesaos/seotools": "^0.17.1",
        "astrotomic/laravel-translatable": "11.5",
        "barryvdh/laravel-debugbar": "3.4.2",
        "barryvdh/laravel-translation-manager": "^0.5.8",
        "beyondcode/laravel-websockets": "1.3",
        "botman/botman": "^2.6",
        "botman/driver-facebook": "^1.11",
        "botman/driver-web": "^1.5",
        "botman/studio-addons": "^1.6",
        "fideloper/proxy": "^4.0",
        "harimayco/laravel-menu": "^1.4",
        "laracasts/flash": "^3.0",
        "laravel-notification-channels/facebook-poster": "2.0.0",
        "laravel/framework": "5.8.*",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.4.0",
        "nilsenj/toastr-laravel": "dev-1.0-stable",
        "pusher/pusher-php-server": "~4.0",
        "renatomarinho/laravel-page-speed": "1.9.0",
        "staudenmeir/eloquent-json-relations": "1.2.4",
        "unisharp/laravel-filemanager": "^2.2",
        "zizaco/entrust": "5.2.x-dev"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "files": [
        "app/Commons/Helpers.php"
    ],
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "BotMan\\Studio\\Providers\\DriverServiceProvider::publishDriverConfigurations"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "BotMan\\Studio\\Providers\\DriverServiceProvider::publishDriverConfigurations"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
